    "version": "0.2.0",
    "configurations": [
        {
            "name": "[Backend]Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/backend"
        },
        {
            "name": "[Web]Dev Server",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}/web",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "dev"
            ]
        }
    ]
}